{
  "name": "onemarshall/contao-aos",
  "description": "Animate content elements on scroll",
  "keywords": [
    "contao",
    "animate",
    "scroll",
    "viewport"
  ],
  "type": "contao-bundle",
  "license": "MIT",
  "require": {
    "php": ">=8.0",
    "contao/core-bundle": "^4.13 || ^5.0"
  },
  "require-dev": {
    "contao/manager-plugin": "^2.0"
  },
  "conflict": {
    "contao/manager-plugin": "<2.0 || >=3.0"
  },
  "autoload": {
    "psr-4": {
      "onemarshall\\AosBundle\\": "src/"
    },
    "classmap": [
      "contao/"
    ],
    "exclude-from-classmap": [
      "contao/config/",
      "contao/dca/",
      "contao/languages/",
      "contao/templates/",
      "contao/themes/"
    ]
  },
  "extra": {
    "contao-manager-plugin": "onemarshall\\AosBundle\\ContaoManager\\Plugin"
  }
}
